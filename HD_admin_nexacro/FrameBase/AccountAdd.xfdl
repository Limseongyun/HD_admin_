          <Column id="qua_minage" type="INT" size="256"/>
          <Column id="qua_maxmoney" type="INT" size="256"/>
          <Column id="qua_minmoney" type="INT" size="256"/>
          <Column id="qua_maxperiod" type="INT" size="256"/>
